{
    "check": {
        "id": "mem-util",
        "name": "Memory utilization",
        "script": "/usr/local/bin/mem.sh -w 20 -c 70",
        "interval": "10s"
    }
}
{
    "check": {
        "id": "cpu-util",
        "name": "CPU utilization",
        "script": "/usr/local/bin/cpu.sh -w 30 -c 80",
        "interval": "10s"
    }
}
