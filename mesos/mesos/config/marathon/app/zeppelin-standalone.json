{
  "id": "zeppelin",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "twistedogic/zeppelin:latest",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 8080, "hostPort": 0, "servicePort": 8083, "protocol": "tcp" },
        { "containerPort": 8081, "hostPort": 0, "servicePort": 8081, "protocol": "tcp" }
      ]
    }
  },
  "instances": 1,
  "cpus": 0.5,
  "mem": 2048,
  "uris": []
}
