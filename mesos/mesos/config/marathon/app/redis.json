{
  "id": "redis",
  "cpus": 0.5,
  "mem": 512,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "redis:latest",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 6379, "hostPort": 0, "servicePort": 6379, "protocol": "tcp" }
      ]
    }
  }
}
