{
  "id": "spark-master",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "twistedogic/spark-hdfs-master:latest",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 8080, "hostPort": 0, "servicePort": 8080, "protocol": "tcp" }
      ]
    }
  },
  "instances": 1,
  "cpus": 0.5,
  "mem": 2048,
  "uris": [],
  "env": {
    "SERF_JOIN_IP": "10.0.0.129"
  }
}
